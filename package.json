{
  "name": "coding-quiz",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "cd server && npm run start",
    "render-build": "npm install && npm run build",
    "seed": "cd server/dist/seeds && node --experimental-json-modules seed.js",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "concurrently \"cd server && npm run build\" \"cd client && npm run build\"",
    "deploy": "npm install && npm run build",
    "test": "npx cypress run --browser electron",
    "test-component": "npx cypress run --component",
    "test-gui": "npx cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16 <21"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cypress": "^13.13.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "vitest": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}